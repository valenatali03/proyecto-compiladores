program {
    
    integer assert_equals(integer obtained, integer expected) extern;

    integer inc(integer x)
    {
        return x + 1;
    }

    bool es_primo(integer n)
    {
      integer i = 2;

        if (n < 2) then
        {
         return false;
        }

        while ((i * i) < (n + 1))
        {
            if ((n % i) == 0) then
            {
               return false;
            }

            i = inc(i);
        }

        return true;
    }

    void main() {
       integer x = 5;

       integer res = 0;

       if (es_primo(x)) then
       {
            res = x;
       }
       else
       {
            res = 0;
       }

       assert_equals(res, 5);
    }
}