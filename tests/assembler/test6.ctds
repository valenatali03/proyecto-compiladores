program {
	integer print_int(integer x) extern;

    void main() {
        integer x = 4;
        integer y = 10;
        integer res = 0;

        if ((x < y) && (y < 20)) then
        {
            res = 1;
        }
        else
        {
            res = 0;
        }

        print_int(res); // 1
    }
}