program {

   integer print_int(integer x) extern;

   integer inc(integer x)
    {
        return x + 1;
    }

    bool es_primo(integer n)
    {
      integer i = 2;

        if (n < 2) then
        {
         return false;
        }

        while ((i * i) < (n + 1))
        {
            if ((n % i) == 0) then
            {
               return false;
            }

            i = inc(i);
        }

        return true;
    }

   integer factorial(integer x)
    {
      if (x < 2) then
      {
         return x;
      }
      
      return x * factorial(x - 1);
    }

    integer fibonacci(integer n)
    {
        if (n < 2) then 
        {
         return n;
        }

        return fibonacci(n - 1) + fibonacci(n - 2);
    }

    bool es_par(integer x)
    {
      return x % 2 == 0;
    }

    void main() {
       integer x = 0;

       integer i = 0;

       while (x < 10)
       {
            print_int(factorial(x));
            x = inc(x);
       } 

       x = 0;

       while (x < 10)
       {
            print_int(fibonacci(x));
            x = inc(x);
       }

       while (i < 20)
       {
            if (es_primo(i)) then
            {
               print_int(i);
            }
            i = inc(i);
       }

       i = 0;

       while (i < 20)
       {
            if (es_par(i)) then
            {
               print_int(i);
            }
            i = inc(i);
       }
    }
}